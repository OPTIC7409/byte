def concatenated = concat("Hello", " ", "World");
console(concatenated)

def original = "Hello, Custom Language!";
def sub = substring(original, 7, 13);
console(sub)

def length = len("Programming");
console(length)

def upperStr = upper("hello");
console(upperStr)

def lowerStr = lower("WORLD");
console(lowerStr)

def replaced = replace("Hello World", "World", "Universe");
console(replaced)

def trimmed = trim("   Hello, Custom Language!   ");
console(trimmed)

def startsWithHello = startsWith("Hello, Custom Language!", "Hello");
console(startsWithHello)

def endsWithWorld = endsWith("Hello, Custom Language!", "World");
console(endsWithWorld)

def parts = split("apple,orange,banana", ",");
console(parts)
